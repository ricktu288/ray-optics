{
  "name": "GRIN slab",
  "version": 5,
  "modules": {
    "ApproxGrinSlab": {
      "numPoints": 2,
      "params": [
        "N=1:1:200:100"
      ],
      "objs": [
        {
          "for": "i=0:1:N-1",
          "type": "Glass",
          "path": [
            {
              "x": "`x_1`",
              "y": "`y_1+(y_2-y_1)i/N`",
              "arc": false
            },
            {
              "x": "`x_1`",
              "y": "`y_1+(y_2-y_1)(i+1)/N`",
              "arc": false
            },
            {
              "x": "`x_2`",
              "y": "`y_1+(y_2-y_1)(i+1)/N`",
              "arc": false
            },
            {
              "x": "`x_2`",
              "y": "`y_1+(y_2-y_1)i/N`",
              "arc": false
            }
          ],
          "refIndex": "`2*sqrt(1-(sqrt(3)/(y_2-y_1)*abs((y_2-y_1)((i+0.5)/N-0.5)))^2)`"
        }
      ]
    }
  },
